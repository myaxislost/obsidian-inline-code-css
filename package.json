{
  "name": "vue-vite-ts-plugin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"vite build --watch --mode development\" \"node scripts/sync.mjs --watch\"",
    "build": "vite build && node scripts/sync.mjs",
    "sync": "node scripts/sync.mjs",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@codemirror/language": "^6.12.1",
    "@codemirror/state": "^6.5.3",
    "@codemirror/view": "^6.39.5",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "chokidar": "^4.0.0",
    "concurrently": "^9.0.0",
    "fs-extra": "^11.2.0",
    "obsidian": "^1.10.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.0",
    "vue-tsc": "^2.0.0"
  }
}
